{
  "name": "miningos-app-ui",
  "version": "0.0.1",
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "engineStrict": true,
  "private": true,
  "type": "module",
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@capacitor/android": "^8.0.2",
    "@capacitor/cli": "^8.0.2",
    "@capacitor/core": "^8.0.2",
    "@reduxjs/toolkit": "^2.2.6",
    "@sentry/cli": "^2.33.1",
    "@sentry/react": "^8.24.0",
    "antd": "^5.19.1",
    "chart.js": "^4.4.9",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "chartjs-plugin-zoom": "^2.0.1",
    "csv-parse": "^6.1.0",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "formik": "^2.4.6",
    "howler": "2.2.4",
    "html-to-image": "^1.11.13",
    "html2canvas": "^1.4.1",
    "jspdf": "^4.0.0",
    "lightweight-charts": "^4.1.7",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.1.5",
    "p-queue": "^8.0.1",
    "p-retry": "^6.2.1",
    "pluralize": "8.0.0",
    "rc-picker": "^4.6.13",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^19.0.0",
    "react-gauge-chart": "^0.5.1",
    "react-infinite-viewer": "^0.28.1",
    "react-number-format": "^5.4.2",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.30.3",
    "react-selecto": "^1.26.3",
    "redux-persist": "6.0.0",
    "styled-components": "^6.1.15",
    "yup": "^1.6.1"
  },
  "scripts": {
    "start": "vite",
    "start:staging": "vite --mode staging",
    "start:demo": "cross-env VITE_USE_MOCKDATA=true vite",
    "start:demo-capture": "cross-env VITE_SAVE_MOCKDATA=true vite",
    "build": "vite build",
    "build:demo": "VITE_USE_MOCKDATA=true vite build",
    "push": "npm run prettier:fix && npm run lint && npm run typecheck && npm run test && git push",
    "preview": "vite preview",
    "preview:demo": "VITE_USE_MOCKDATA=true vite preview",
    "typecheck": "NODE_OPTIONS='--max-old-space-size=4096' tsc --noEmit",
    "build:staging": "vite build --mode staging",
    "preview:staging": "vite preview --mode staging",
    "build-with-sentry": "npm run build && npm run sentry:sourcemaps",
    "build-with-sentry:staging": "npm run build:staging && npm run sentry:sourcemaps",
    "test": "vitest run --config vitest.config.js --maxConcurrency=5",
    "test:watch": "vitest --watch --config vitest.config.js",
    "test:coverage": "vitest run --coverage --config vitest.config.js",
    "lint": "eslint --cache --cache-location .eslintcache --debug --max-warnings 0",
    "lint:fix": "eslint --fix --cache --cache-location .eslintcache --max-warnings 0",
    "prettier": "prettier ./src --check",
    "prettier:fix": "prettier ./src --write",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org tether-5r --project miningos-javascript-react ./build && sentry-cli sourcemaps upload --org tether-5r --project miningos-javascript-react ./build",
    "sentry:create-config": "node ./scripts/createSentryConfig.js",
    "analyze": "BUNDLE_ANALYZE=true vite build",
    "check-deps": "madge --circular --extensions js,jsx,ts,tsx ./src",
    "verify-build": "node scripts/verifyBuild.js",
    "verify-build:staging": "node scripts/verifyBuild.js staging",
    "verify-build:production": "node scripts/verifyBuild.js production",
    "sanitize-mockdata": "node scripts/sanitizeMockdata.cjs",
    "android:sync": "npx cap sync android",
    "android:open": "npx cap open android",
    "android:run": "npx cap run android",
    "android:build": "npm run build && npm run android:sync"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@eslint/js": "^9.35.0",
    "@storybook/addon-essentials": "8.4.7",
    "@storybook/addon-interactions": "8.4.7",
    "@storybook/addon-links": "8.4.7",
    "@storybook/addon-onboarding": "8.4.7",
    "@storybook/react": "8.4.7",
    "@storybook/react-webpack5": "8.4.7",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/howler": "^2.2.12",
    "@types/lodash": "^4.17.20",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.8.0",
    "@types/prop-types": "^15.7.15",
    "@types/qs": "^6.9.17",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.8",
    "babel-plugin-date-fns": "^2.0.0",
    "babel-plugin-import": "^1.13.8",
    "babel-plugin-react-compiler": "^1.0.0",
    "babel-plugin-styled-components": "^2.1.4",
    "cross-env": "^10.1.0",
    "eslint": "^9.35.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-lodash": "7.4.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-testing-library": "^7.8.1",
    "jsdom": "^26.0.0",
    "lightningcss": "^1.30.2",
    "madge": "^8.0.0",
    "prettier": "^3.5.3",
    "prop-types": "15.8.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "storybook": "^8.2.2",
    "typescript": "^5.9.3",
    "vite": "^6.3.2",
    "vitest": "^3.0.8"
  }
}
